        "confidence_threshold": 0.8
      },
      "tax_return": {
        "extractor": "TaxReturnExtractor",
        "required_fields": [
          "tax_year",
          "taxpayer_name",
          "filing_status",
          "adjusted_gross_income"
        ],
        "optional_fields": [
          "spouse_name",
          "wages",
          "total_tax",
          "refund_amount"
        ],
        "confidence_threshold": 0.9
      }
    },
    "field_mappings": {
