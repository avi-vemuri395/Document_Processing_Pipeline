{
  "sba_generator": {
    "min_loan_amount": 25000,
    "max_loan_amount": 500000,
    "revenue_multiplier": 3.5,
    "profit_margin_range": [0.05, 0.25],
    "statement_months": 12,
    "business_types": [
      "Restaurant",
      "Retail Store", 
      "Manufacturing",
      "Construction",
      "Professional Services",
      "Technology",
      "Healthcare",
      "Automotive Services",
      "Real Estate",
      "Consulting"
    ],
    "template_dir": "templates/sba"
  },
  "accuracy_reporter": {
    "numeric_tolerance": 0.01,
    "currency_tolerance": 0.01,
    "text_similarity_threshold": 0.8
  },
  "test_data_manager": {
    "max_datasets": 50,
    "retention_days": 30
  },
  "test_scenarios": [
    {
      "name": "small_business_loans",
      "num_applications": 5,
      "config": {
        "min_loan_amount": 25000,
        "max_loan_amount": 100000,
        "business_types": ["Restaurant", "Retail Store"],
        "statement_months": 6
      }
    },
    {
      "name": "medium_business_loans", 
      "num_applications": 5,
      "config": {
        "min_loan_amount": 100000,
        "max_loan_amount": 300000,
        "business_types": ["Manufacturing", "Professional Services"],
        "statement_months": 12
      }
    },
    {
      "name": "large_business_loans",
      "num_applications": 3,
      "config": {
        "min_loan_amount": 300000,
        "max_loan_amount": 500000,
        "business_types": ["Technology", "Healthcare"],
        "statement_months": 18
      }
    },
    {
      "name": "high_profit_margin",
      "num_applications": 3,
      "config": {
        "min_loan_amount": 50000,
        "max_loan_amount": 200000,
        "profit_margin_range": [0.15, 0.35],
        "business_types": ["Technology", "Professional Services"]
      }
    },
    {
      "name": "low_profit_margin",
      "num_applications": 3,
      "config": {
        "min_loan_amount": 50000,
        "max_loan_amount": 200000,
        "profit_margin_range": [0.02, 0.08],
        "business_types": ["Restaurant", "Retail Store"]
      }
    }
  ],
  "benchmark_settings": {
    "default_num_applications": 10,
    "accuracy_thresholds": {
      "excellent": 0.9,
      "good": 0.8,
      "acceptable": 0.6,
      "poor": 0.4
    },
    "performance_thresholds": {
      "fast": 2.0,
      "acceptable": 5.0,
      "slow": 10.0
    }
  }
}